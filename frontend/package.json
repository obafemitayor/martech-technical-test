{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^3.27.0",
    "@emotion/react": "^11.14.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@zag-js/checkbox": "^1.24.2",
    "axios": "^1.12.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-intl": "^7.1.11",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "eject": "react-scripts eject"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!(@ark-ui|@chakra-ui|@zag-js|proxy-compare|uqr|axios)/)"
    ],
    "moduleNameMapper": {
      "^@ark-ui/react/anatomy$": "<rootDir>/node_modules/@ark-ui/react/dist/components/anatomy.cjs",
      "^@ark-ui/react/factory$": "<rootDir>/node_modules/@ark-ui/react/dist/components/factory.cjs",
      "^@ark-ui/react/environment$": "<rootDir>/node_modules/@ark-ui/react/dist/providers/environment/index.cjs",
      "^@ark-ui/react/locale$": "<rootDir>/node_modules/@ark-ui/react/dist/providers/locale/index.cjs",
      "^@ark-ui/react/(.*)$": "<rootDir>/node_modules/@ark-ui/react/dist/components/$1/index.cjs"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ark-ui/react": "^5.25.0",
    "@types/uuid": "^10.0.0",
    "ajv": "^6.12.6",
    "ajv-keywords": "^3.5.2",
    "core-js": "^3.45.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
